<settings>
	<pluginGroups>
		<pluginGroup>com.atlassian.maven.plugins</pluginGroup>
	</pluginGroups>
	<mirrors>
    <mirror>
      <!--This sends everything else to /public -->
      <id>nexus</id>
      <mirrorOf>*</mirrorOf>
      <url>http://uxpapgigjenk01.dstcorp.net:8081/nexus/content/groups/public</url>
    </mirror>
  </mirrors>
    <profiles>
        <profile>
            <repositories>
				<repository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>central</id>
                    <name>central-proxy</name>
                    <url>http://uxpapgigjenk01.dstcorp.net:8081/nexus/content/repositories/central/</url>
                </repository>
                <repository>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                    <id>apache</id>
                    <name>apache-proxy</name>
                    <url>http://uxpapgigjenk01.dstcorp.net:8081/nexus/content/repositories/apache-snapshots/</url>
                </repository>
				<repository>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                    <id>codehaus</id>
                    <name>codehaus-proxy</name>
                    <url>http://uxpapgigjenk01.dstcorp.net:8081/nexus/content/repositories/codehaus-snapshots/</url>
                </repository>
				<repository>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>gig-releases</id>
					<name>Gig Releases</name>
					<url>http://uxpapgigjenk01.dstcorp.net:8081/nexus/content/repositories/releases/</url>
				</repository>
				<repository>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
					<id>gig-snapshots</id>
					<name>Gig Snapshots</name>
					<url>http://uxpapgigjenk01.dstcorp.net:8081/nexus/content/repositories/snapshots/</url>
				</repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>central</id>
                    <name>plugins-release</name>
                    <url>http://artifacts-scm.dstcorp.net/artifactory/plugins-release</url>
                </pluginRepository>
                <pluginRepository>
                    <snapshots />
                    <id>snapshots</id>
                    <name>plugins-snapshot</name>
                    <url>http://artifacts-scm.dstcorp.net/artifactory/plugins-snapshot</url>
                </pluginRepository>
            </pluginRepositories>
            <id>artifactory</id>
        </profile>
    </profiles>
</settings>

